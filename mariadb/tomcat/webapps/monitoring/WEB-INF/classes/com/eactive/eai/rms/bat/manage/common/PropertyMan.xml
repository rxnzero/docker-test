<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="BatProperty">
	
	<statement id="propertyGroupDetail" resultClass="java.util.HashMap">
	 	SELECT  PRPTYGROUPID AS "PRPTYGROUPID", PRPTYGROUPNAME AS "PRPTYGROUPNAME"
         		FROM  $schemaId$.TSEAIEA09
		WHERE	PrptyGroupId = #prptyGroupId#
	</statement> 	
	
	<statement id="propertyDetail" resultClass="java.util.HashMap">
	 	SELECT  PRPTYID AS "PRPTYID", PRPTYDETAIL AS "PRPTYDETAIL"
         		FROM  $schemaId$.TSEAIEA10
		WHERE	PrptyGroupId = #prptyGroupId#
	</statement>  

	<statement id="update" parameterClass="java.util.HashMap" resultClass="java.lang.Integer">
		UPDATE 	$schemaId$.TSEAIEA09 
				SET PrptyGroupName = #prptyGroupName# 
		WHERE 	PrptyGroupId = #prptyGroupId#
	</statement>
	
	<statement id="insert" parameterClass="java.util.HashMap" resultClass="java.lang.Integer">
		INSERT 	INTO $schemaId$.TSEAIEA09(
				PrptyGroupId, PrptyGroupName) 
				VALUES(#prptyGroupId#, #prptyGroupName#)
	</statement>
	
	<statement id="insertProperty" parameterClass="java.util.HashMap" resultClass="java.lang.Integer">
		INSERT 	INTO $schemaId$.TSEAIEA10(
				PrptyGroupId, PrptyId, PrptyDetail) 
				VALUES(#prptyGroupId#, #prptyId# ,#prptyDetail#)
	</statement>
	
	<statement id="delete" parameterClass="java.util.HashMap" resultClass="java.lang.Integer">
		DELETE 	FROM $schemaId$.TSEAIEA09 WHERE PrptyGroupId = #prptyGroupId#
	</statement>

	<statement id="deleteProperty" parameterClass="java.util.HashMap" resultClass="java.lang.Integer">
		DELETE 	FROM $schemaId$.TSEAIEA10 WHERE PrptyGroupId = #prptyGroupId#
	</statement>
	
</sqlMap>
