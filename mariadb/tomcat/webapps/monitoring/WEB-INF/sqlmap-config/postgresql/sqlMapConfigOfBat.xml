    resource="com/eactive/eai/rms/bat/manage/common/PropertyMan-postgresql.xml" />

  <sqlMap
    resource="com/eactive/eai/rms/bat/manage/common/ServerMan.xml" />
  <sqlMap
    resource="com/eactive/eai/rms/bat/manage/common/ServerMan-postgresql.xml" />

  <sqlMap
    resource="com/eactive/eai/rms/bat/manage/common/LogDeleteMan.xml" />
    
    
  <sqlMap
    resource="com/eactive/eai/rms/bat/admin/rule/LayoutSync-postgresql.xml" />

