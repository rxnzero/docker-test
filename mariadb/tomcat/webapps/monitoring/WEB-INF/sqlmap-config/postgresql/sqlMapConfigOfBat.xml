    resource="com/eactive/eai/rms/bat/manage/cron/CronMan-postgresql.xml" />
    
  <!-- BAT형상관리 파일 loader -->    
  <sqlMap
    resource="com/eactive/eai/rms/bat/loader/BatLoader.xml" />
